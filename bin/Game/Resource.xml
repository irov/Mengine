<DataBlock>
	<Resource Name = "output" Type = "ResourceImageMNG" >
		<File Path = "output.mng" />
	</Resource>

	<Resource Name = "default" Type = "ResourceImageMNG" >
		<File Path = "Game/Arrows/Default/Default.mng" />
	</Resource>
		
	<Resource Name = "output_cng" Type = "ResourceImageCNG" >
		<File Path = "output.cng" />
	</Resource>	

	<Resource Name = "output_cngAnimation" Type = "ResourceAnimation" >
		<ImageMap Name = "output_cng" />
		<Sequences>
			<Sequence Frame = "1" Delay = "50" />
			<Sequence Frame = "2" Delay = "50" />
			<Sequence Frame = "3" Delay = "50" />
			<Sequence Frame = "4" Delay = "50" />
			<Sequence Frame = "5" Delay = "50" />
		</Sequences>
	</Resource>
</DataBlock>

